{
  "extends": ["tslint-react", "prettier-tslint"],
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "semicolon": false,
    "jsx-no-multiline-js": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
