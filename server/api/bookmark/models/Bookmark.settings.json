{
  "connection": "default",
  "collectionName": "bookmark",
  "info": {
    "name": "bookmark",
    "description": "A link witch belongs to a user"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "link": {
      "type": "string",
      "required": true
    },
    "user": {
      "model": "user",
      "via": "bookmarks",
      "plugin": "users-permissions"
    }
  }
}